{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowImportingTsExtensions": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "App.tsx",
    "index.ts",
    "shims/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.backup.tsx",
    "**/*.backup.ts"
  ]
}
